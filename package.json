{
  "name": "qmates-bookmarks",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/index.js",
  "engines": {
    "node": "^22.16.0",
    "npm": "^10.9.2"
  },
  "scripts": {
    "test": "npm run build && LOG_LEVEL=SILENT vitest run",
    "coverage": "npm run build && LOG_LEVEL=SILENT vitest run --coverage",
    "start": "node .",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TS,App\" \"tsc -w\" \"node --watch . | pino-pretty\"",
    "format": "prettier --write",
    "build": "rm -rf ./dist/*; tsc",
    "prepare": "husky || true"
  },
  "author": "Alessio Coser",
  "dependencies": {
    "@fastify/sensible": "^6.0.3",
    "fastify": "^5.3.3",
    "ghtml": "^3.1.3",
    "pino": "^9.7.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
